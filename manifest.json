{
  "applications": {
    "gecko": {
      "id": "password-exporter-webextension@mozilla.org"
    }
  },
  "browser_action": {
      "default_icon": "key.png",
      "default_title": "Password Exporter"
  },
  "background": {
     "scripts": ["background.js"]
  },
  "description": "Allows you to export your passwords",
  "manifest_version": 2,
  "name": "Password Exporter",
  "version": "0.1",
  "permissions": [
    "logins",
    "experiments.logins",
    "<all_urls>",
    "downloads"
  ],
  "icons": {
    "128": "key.png"
  }
}
